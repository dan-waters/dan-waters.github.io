var $title,a;jQuery(function(n){n("#df_header_search input.searchInput").val("Search");n("#df_header_search .searchInput").focus(function(){n(this).val()=="Search"&&n(this).val("")});n("#df_header_search .searchInput").blur(function(){n(this).val()==""&&n(this).val("Search")});n("div.commentsForm")&&n('.commentsForm input[type="submit"]').attr("value","Submit Comment");var t=["January","February","March","April","May","June","July","August","September","October","November","December"];n(".BlogFeatureList .blogDate").each(function(){n(this).html(n(this).html().replace(/(date posted:|Monday|Tuesday|Wednesday|Thursday|Friday|Saturday|Sunday)/ig,""));var r=n(this).html(),i=new Date(r),u=i.getDate()+" "+t[i.getMonth()]+" "+i.getFullYear();n(this).html(n(this).html().replace(r,u))});n(".postInformation").each(function(){var t=n(this).html();t=t.trim();var i=t.substring(0,t.length-10).trim(),r=t.substring(t.length-4,t.length),u=t.substring(t.length-7,t.length-5),f=t.substring(t.length-10,t.length-8),e="Created by <b>"+i+"<\/b> on <b>"+f+"/"+u+"/"+r+"<\/b>";n(this).html(e)})})(".EventsCalendar .time").each(function(){var r=jQuery(this).html().trim(),n=r.split(" "),u=n.pop();n.pop();var t=n.pop(),f=n.pop(),i=n.pop();n.pop();var e=n.join(" "),o=new Date(i+","+f+","+t),s=e+" at "+u+" on "+parseInt(i,10)+" "+month_text[o.getMonth()]+"  "+t;jQuery(this).html(s)});$title="";jQuery("div.ceDetails")&&(jQuery(".ceTitle").css("display","none"),$title=jQuery(".ceTitle").html(),jQuery(".page_title").prepend($title));jQuery("div.blogBody")&&(jQuery(".blog h2").css("display","none"),$title=jQuery(".blog h2").html(),jQuery(".page_title").prepend($title));location.href.indexOf("/sitemap.aspx")>-1&&($title="Sitemap",jQuery(".page_title").prepend($title));location.href.indexOf("/SearchResults.aspx")>-1&&($title="Search results",jQuery(".page_title").prepend($title));jQuery("#df_login_body").length&&(a=0,$title="Login",jQuery(".page_title").prepend($title),jQuery("#df_login_body table").each(function(n){jQuery(this).attr("class","login_table_"+n)}),jQuery("#df_login_body tr").each(function(n){jQuery(this).attr("class","login_tr_"+n)}),jQuery("#df_login_body td").each(function(n){jQuery(this).attr("class","login_td_"+n)}))